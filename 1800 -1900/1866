using System; 

class URI {

    static void Main(string[] args) { 

        int qtd = Convert.ToInt32(Console.ReadLine()), num;
        
        for(int i=0; i<qtd; i++){
            num = Convert.ToInt32(Console.ReadLine());
            if(num % 2 == 1){
                Console.WriteLine("1");
            } else{
                Console.WriteLine("0");
            }
        }
    }

}
